menuconfig NTURT_TM
    depends on NTURT
    bool "Telemetry module"
    depends on SYS_HASH_FUNC32
    depends on SYS_HASH_MAP
    select NTURT_MSG
    help
      ""

if NTURT_TM

config NTURT_TM_INIT_PRIORITY
    int "Telemetry module init priority"
    default 50
    help
      Telemetry module initialization priority.

endif # NTURT_TM
